{
  "name": "vue-insta-carousel-3d",
  "version": "1.0.14",
  "description": "Fetch Instagram feed components based on Vue.",
  "author": "Imran Basha",
  "main": "dist/vue-insta-carousel-3d.min.js",
  "keywords": [
    "component",
    "instagram",
    "feed",
    "vue",
    "vue.js"
  ],
  "scripts": {
    "serve": "vue serve src/vue-insta-carousel-3d.vue",
    "lint": "./node_modules/.bin/eslint --ext .js,.vue src",
    "build": "./node_modules/.bin/webpack --hide-modules -p --progress"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.0",
    "babel-eslint": "^10.0.3",
    "lodash": "^4.17.19",
    "style-loader": "^1.1.3",
    "vue-carousel-3d": "^0.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/preset-env": "^7.7.5",
    "babel-loader": "^8.0.6",
    "babel-plugin-lodash": "^3.3.4",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.1",
    "eslint": "6.7.2",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "6.0.1",
    "vue": "^2.6.10",
    "vue-loader": "^15.7.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "babel": {
    "plugins": [
      "lodash"
    ],
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": false,
          "forceAllTransforms": true,
          "targets": "> 0.25%, not dead"
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": [
      "plugin:vue/recommended",
      "standard"
    ],
    "parserOptions": {
      "ecmaVersion": 2017
    },
    "rules": {
      "vue/require-v-for-key": 0
    }
  }
}
